{{!< default}}
<div class="h-[96px] w-screen-full bg-slate-700 text-slate-100 flex justify-center items-center">
	<div class="text-center">PLACEHOLDER ADVERTISEMENT 96px height 1280px width</div>
</div>

<div class="gh-page">
    <div class="gh-container">

        <div class="gh-postfeed">
        {{#foreach posts}}

            {{> "card" page=../pagination.page }} {{!-- partials/card.hbs --}}

        {{/foreach}}
        </div>

        {{!-- If JS isn't enabled, fallback to standard pagination --}}
        <noscript>
            <div class="gh-pagination">
                {{pagination}}
            </div>
        </noscript>
    </div>
</div>
